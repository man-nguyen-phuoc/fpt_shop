<%- if @error %>
<p><%= @error %></p>
<% else %>
  <table>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Created at</th>
      <th>Updated at</th>
      <th>Action</th>
    </tr>
      <tr>
        <td><%= @manufacture.id %></td>
        <td><%= @manufacture.name %></td>
        <td><%= @manufacture.created_at %></td>
        <td><%= @manufacture.updated_at %></td>
        <td>
          <%= button_to "Deleted", manufacture_path(@manufacture), method: :delete %>
        </td>
      </tr>  
  </table>
<%end%>